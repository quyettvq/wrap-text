{
  "name": "wraptext.js",
  "version": "1.1.3",
  "description": "Wrap text, supports Unicode and non-monospace fonts",
  "main": "./dist/unicode.js",
  "types": "./dist/unicode.d.ts",
  "exports": {
    ".": {
      "types": "./dist/unicode.d.ts",
      "module": "./dist/unicode.mjs",
      "import": "./dist/unicode.mjs",
      "default": "./dist/unicode.js"
    },
    "./unicode": {
      "types": "./dist/unicode.d.ts",
      "module": "./dist/unicode.mjs",
      "import": "./dist/unicode.mjs",
      "default": "./dist/unicode.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "module": "./dist/base.mjs",
      "import": "./dist/base.mjs",
      "default": "./dist/base.js"
    },
    "./noKern": {
      "types": "./dist/noKern.d.ts",
      "module": "./dist/noKern.mjs",
      "import": "./dist/noKern.mjs",
      "default": "./dist/noKern.js"
    }
  },
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "rollup": "^2.74.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quyettvq/wraptext.git"
  },
  "keywords": [
    "wrap text",
    "line break",
    "word wrap",
    "canvas wrap text"
  ],
  "author": "Tran Van Quyet <quyettvq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quyettvq/wraptext/issues"
  },
  "homepage": "https://github.com/quyettvq/wraptext#readme"
}
